public class Man {

void drawMan() {  
size(400, 400); } 
void draw() { 
line(300, 200, 100, 200); //hand
line(200, 200, 200, 340);//body
line(200, 341, 120, 390);//right foot
line(200, 341, 270, 390);//left foot


drawCircle(); 
} 
void drawCircle() {  
ellipse(200, 162, 70, 70); 
ellipse(185,155,10,10); //right eye
ellipse(215,155,10,10);//left eye
arc(195,170,200,190,215,170);//smile (why dont work?)
}
}
